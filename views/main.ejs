<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Main</title>
  </head>
  <body>
    <header>
      <h1>Home</h1>
      <a href="/logout">Logout</a>
      <a href="/main/post">New Post</a>
    </header>
    <main>
      <ul>
        <% posts.forEach((post) => { %>
        <a href="main/<%=post._id%>""><li><%=post.postTitle %></li></a>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
